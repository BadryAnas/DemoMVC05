@model EmployeeDto

@{
    ViewData["Title"] = "Details Of Employee";
}


<br />
<br />

<form asp-route-id="@Model.Id">
    <div class="form-group">
        <label class="col-form-label" asp-for="Age"></label>
        <input type="text" asp-for="Age" class="form-control" readonly />
    </div>


    <div class="form-group">
        <label class="col-form-label" asp-for="DepartmentId"></label>
        <input type="text" asp-for="DepartmentId" class="form-control" readonly />
    </div>


    <div class="form-group">
        <label class="col-form-label" asp-for="Salary"></label>
        <input type="text" asp-for="Salary" class="form-control" readonly />
    </div>


    <div class="form-group">
        <label class="col-form-label" asp-for="Email"></label>
        <input type="text" asp-for="Email" class="form-control" readonly />
    </div>


    <div class="form-group">
        <label class="col-form-label" asp-for="HiringDate"></label>
        <input type="text" asp-for="HiringDate" class="form-control" readonly />
    </div>


    <div class="form-group">
        <label class="col-form-label" asp-for="PhoneNumber"></label>
        <input type="text" asp-for="PhoneNumber" class="form-control" readonly />
    </div>


    <div class="form-group">
        <label class="col-form-label" asp-for="Address"></label>
        <input type="text" asp-for="Address" class="form-control" readonly />
    </div>


    <div class="form-group">
        <label class="col-form-label" asp-for="Name"></label>
        <input type="text" asp-for="Name" class="form-control" readonly />
    </div>

    <div class="form-group">
        <label class="col-form-label" asp-for="CreateAt"></label>
        <input type="text" asp-for="CreateAt" class="form-control" readonly />
    </div>

    <div class="text-center">
        <a asp-controller="Employee" asp-action="Index" class="btn btn-light">Back To List</a>
    </div>
</form>



